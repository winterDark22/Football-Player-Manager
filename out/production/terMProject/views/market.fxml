<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MarketController">
   <children>
      <HBox prefHeight="38.0" prefWidth="780.0">
         <children>
            <Button fx:id="homeButton" mnemonicParsing="false" onAction="#homeAction" prefHeight="27.0" prefWidth="61.0" styleClass="search-buttton" stylesheets="@../values/style.css" text="Home" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" right="5.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Gill Sans MT" size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <VBox alignment="TOP_CENTER" layoutY="43.0" prefHeight="556.0" prefWidth="388.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="388.0" styleClass="box-background" stylesheets="@../values/style.css" text="Selling List">
               <font>
                  <Font name="Gill Sans MT Condensed" size="22.0" />
               </font></Label>
            <ScrollPane prefHeight="535.0" prefWidth="390.0" styleClass="transparent" stylesheets="@../values/style.css">
               <content>
                  <GridPane fx:id="sellGrid" prefHeight="0.0" prefWidth="12.0">
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="392.0" layoutY="46.0" prefHeight="550.0" prefWidth="388.0">
         <children>
            <Label alignment="CENTER" prefHeight="38.0" prefWidth="388.0" styleClass="box-background" stylesheets="@../values/style.css" text="Buying List">
               <font>
                  <Font name="Gill Sans MT Condensed" size="22.0" />
               </font></Label>
            <ScrollPane prefHeight="537.0" prefWidth="390.0" styleClass="transparent" stylesheets="@../values/style.css">
               <content>
                  <GridPane fx:id="buyGrid" prefHeight="0.0" prefWidth="3.0">
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
