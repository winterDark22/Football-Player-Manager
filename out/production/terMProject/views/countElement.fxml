<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="30.0" prefWidth="350.0" styleClass="box-background" stylesheets="@../values/style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.elementController">
   <children>
      <HBox alignment="CENTER" prefHeight="35.0" prefWidth="350.0">
         <children>
            <Label fx:id="countryName" prefHeight="33.0" prefWidth="220.0">
               <font>
                  <Font name="Gill Sans MT Condensed" size="23.0" />
               </font>
            </Label>
            <Label fx:id="countryCount" prefHeight="33.0" prefWidth="115.0">
               <font>
                  <Font name="Gill Sans MT Condensed" size="23.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
